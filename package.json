{
  "name": "@arimethys/ng2-img-max",
  "version": "2.2.5",
  "description": "Angular module to resize images down to a certain width and height or to reduce the quality to fit a certain maximal filesize - all in the browser. Just an update of angular/core requirement",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arimethys/ng2-img-max.git"
  },
  "license": "MIT",
  "main": "./dist/bundles/ng2-img-max.umd.js",
  "module": "./dist/ng2-img-max.js",
  "typings": "./dist/ng2-img-max.d.ts",
  "scripts": {
    "transpile": "ngc",
    "package": "rollup -c",
    "minify": "uglifyjs dist/bundles/ng2-img-max.umd.js --screw-ie8 --compress --mangle --comments --output dist/bundles/ng2-img-max.umd.min.js",
    "build": "npm run transpile && npm run package && npm run minify"
  },
  "author": "Arimethys <arimethys@gmail.com>",
  "keywords": [
    "angular",
    "angular2",
    "img-max",
    "maximal",
    "size",
    "limit",
    "resize"
  ],
  "dependencies": {
    "exifr": "^4.3.4",
    "ng2-pica": "^1.3.1"
  },
  "peerDependencies": {
    "@angular/core": "^11.2.13",
    "core-js": ">=2.4.1",
    "rxjs": "^6.5.3"
  },
  "devDependencies": {
    "@angular/common": "^11.2.13",
    "@angular/compiler": "^11.2.13",
    "@angular/compiler-cli": "^11.2.13",
    "@angular/core": "^11.2.13",
    "@angular/platform-browser": "^11.2.13",
    "@angular/platform-browser-dynamic": "^11.2.13",
    "@angular/router": "^11.2.13",
    "@types/node": ">=7.0.5",
    "core-js": "^2.4.1",
    "reflect-metadata": "^0.1.3",
    "rollup": "^0.41.4",
    "rxjs": "^6.5.3",
    "tslint": "^5.20.1",
    "typescript": "~4.1.0",
    "uglify-js": "^2.7.5",
    "zone.js": "^0.11.3"
  },
  "bugs": {
    "url": "https://github.com/arimethys/ng2-img-max/issues"
  },
  "homepage": "https://github.com/arimethys/ng2-img-max#readme"
}
